{
  "description": "Scrolling through tabs using a rotary encoder",
  "manifest_version": 2,
  "name": "webtuner",
  "version": "1.0",
  "icons": {
    "48": "icons/message.svg"
  },
  "applications": {
    "gecko": {
      "id": "webtuner@cursed.ee",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/message.svg"
  },
  "permissions": [
    "nativeMessaging",
    "sessions"
  ]
}
